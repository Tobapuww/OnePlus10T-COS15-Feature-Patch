    <!-- tianli@MULTIMEDIA.AUDIOSERVER.BASE.2020/9/1. Add for voice wakeup features start-->
    <oplus-feature name="oplus.software.audio.voice_wakeup_support"/>
    <oplus-feature name="oplus.software.audio.voice_wakeup_3words_support"/>
    <!-- tianli@MULTIMEDIA.AUDIOSERVER.BASE.2020/9/1. Add for voice wakeup features end-->
    <!-- Helongbao@MULTIMEDIA.VIBRATE, 2020/09/07, Add for FourD Vibrate feature start -->
    <oplus-feature name="oplus.software.haptic_vibrator_v1.support"/>
    <!-- Helongbao@MULTIMEDIA.VIBRATE, 2020/09/07, Add for FourD Vibrate feature end -->
    <!-- yang.y.liu@GAMEECO.JoyStick, 2020/09/07, Add for joystick feature start -->
    <oplus-feature name="oplus.software.joystick.game_joystick_support"/>
    <!-- end -->
    <!-- yang.y.liu@GAMEECO.Touch, 2020/09/07, Add for touch adjuster feature start -->
    <oplus-feature name="oplus.software.game.touch_adjuster_support"/>
    <!-- end -->
    <!-- yang.y.liu@GAMEECO.JoyStick, 2020/11/30, Add for joystick feature start -->
    <oplus-feature name="oplus.software.joystick.game_joystick_support"/>
    <!-- end -->
    <!-- yang.y.liu@GAMEECO.Touch, 2020/11/30, Add for touch adjuster feature start -->
    <oplus-feature name="oplus.software.game.touch_adjuster_support"/>
    <!-- end -->
    <!-- Add for dual light sensor support-->
    <oplus-feature name="oplus.software.display.dual_lightsensor_support"/>
    <!-- end -->
    <!-- #ifdef OPLUS_FEATURE_EAR_PROTECTION -->
    <!-- Rui.Zhang@MULTIMEDIA.AUDIOSERVER.FRAMEWORK.2021/7/29. Add for hearing health feature -->
    <oplus-feature name="oplus.software.audio.hearing_health_support"/>
    <!-- #endif /* OPLUS_FEATURE_EAR_PROTECTION */ -->
    <!-- #ifdef OPLUS_FEATURE_SPL_LIMIT -->
    <!-- Rui.Zhang@MULTIMEDIA.AUDIOSERVER.FRAMEWORK.2021/7/29. Add for spl limit -->
    <oplus-feature name="oplus.software.audio.spl_limit_support"/>
    <!-- #endif /* OPLUS_FEATURE_SPL_LIMIT */ -->

    <!-- Liurongbo@MULTIMEDIA, 2022/10/19, Add for GCP -->
    <oplus-feature name="oplus.software.display.game_color_plus_v2_support" />

    <!-- #ifdef OPLUS_FEATURE_EYE_PROTECT -->
