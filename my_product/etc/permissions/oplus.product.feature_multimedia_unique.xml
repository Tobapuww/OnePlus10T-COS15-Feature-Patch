    <oplus-feature name="oplus.software.display.proximity_sensor_support_for_intelligent_color_temperature"/>
    <!-- #endif /* OPLUS_FEATURE_EYE_PROTECT */ -->

    <!-- Lei.Su@MULTIMEDIA.AUDIOSERVER.BASE, 2024/02/06, add for multi-application volume adjustment -->
    <oplus-feature name="oplus.software.multi_app.volume.adjust.support"/>
    <oplus-feature name="oplus.software.display.sharpness_switch"/>
    <oplus-feature name="oplus.hardware.audio.voice_isolation_support"/>
